---
interface Props {
    image: string;
    alt: string;
}

const { image, alt } = Astro.props;
---
<img src={image} alt={alt} class="w-full object-cover aspect-[4/2.5] bars-mask">

<style>
    .bars-mask {
        mask-image: url(/images/bars.svg);
        mask-repeat: no-repeat;
        mask-size: contain;
    }
</style>