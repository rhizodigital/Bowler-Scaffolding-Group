---
import { Image } from 'astro:assets';
interface Props {
    image: ImageMetadata;
    alt: string;
}

const { image, alt } = Astro.props;
---
<Image src={image} alt={alt} class="w-full object-cover aspect-[4/2.6] bars-mask">



<style>
    .bars-mask {
        mask-image: url(/images/bars.svg);
        mask-repeat: no-repeat;
        mask-size: contain;
    }
</style>